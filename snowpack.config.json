{
  "extends": "@snowpack/app-scripts-react",
  "plugins": [
    [
      "@snowpack/plugin-react-refresh",
      "@snowpack/plugin-optimize",
      [
        "@snowpack/plugin-webpack",
        {
          "outputPattern": {
            "js": "bundle.js"
          }
        }
      ]
    ]
  ],
  "routes": [{ "match": "routes", "src": ".*", "dest": "/index.html" }],
  "devOptions": {
    "port": 8082
  }
}
